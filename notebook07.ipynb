    "fp = open(\"pc_writetest.txt\", \"w\")\n",
    "while True:\n",
    "    text = input(\"Enter a line to write to the file, or press ENTER to quit! \")\n",
    "    if text == \"\":\n",
    "        break\n",
    "    fp.write(text)\n",
    "    fp.close()\n",
    "\n",
    "print(\"Here is what was written into the file:\")\n",
    "print(\"---------------------------------------\")\n",
    "fp = open(\"pc_writetest.txt\")\n",
    "content = fp.read()\n",
    "fp.close()\n",
    "print(content)"
   ]
